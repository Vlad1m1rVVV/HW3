1)
function drawTriangle(height, symbol) {
    for (let i = 1; i <= height; i++) {
        let row = '';
        for (let j = 0; j < i; j++) {
            row += symbol;
        }
        console.log(row);
    }
}
drawTriangle(5, '*');



2)
function pow(x, y) {
    let result = 1;
    for (let i = 0; i < y; i++) {
        result *= x;
    }
    return result;
}


console.log(pow(2, 3));
